public class Main {
int x;
// Constructor with a parameter
public Main(int x) {
this.x = x;
}
// Call the constructor
public static void main(String[] args) {
Main myObj = new Main(5);
System.out.println("Value of x = " + myObj.x);
}
}







class Vehicle { 
int maxSpeed = 120;
}
class Car extends Vehicle { 
int maxSpeed = 180;
void display()
{

System.out.println("Maximum Speed: "
+ super.maxSpeed);
} }
public class Test { 
public static void main(String[] args)
{
Car small = new Car();
small.display();
}
}
